---
import type { RouterFilter } from "@trampo/routes";

import FooterCopyright from "./FooterCopyright.astro";
import FooterLinks from "./FooterLinks.astro";
import FooterNavigation from "./FooterNavigation.astro";
import FooterPartners from "./FooterPartners.astro";

import "./Footer.scss";

const filter: RouterFilter = { isFooter: true };
---

<footer class="footer">
  <FooterLinks filter={filter} />
  <FooterNavigation filter={filter} />
  <FooterPartners />
  <FooterCopyright />
</footer>
