{
  "name": "common",
  "version": "0.0.1",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && npm run fixup",
    "fixup": "echo {\"type\": \"commonjs\"} > dist/cjs/package.json && echo {\"type\": \"module\"} > dist/mjs/package.json",
    "dev": "npm run build && concurrently \"tsc -p tsconfig.json --watch\" \"tsc -p tsconfig-cjs.json --watch\""
  },
  "dependencies": {
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/node": "^18.11.17",
    "concurrently": "^7.6.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.4"
  },
  "type": "module"
}
