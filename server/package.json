{
  "name": "trampolineintercite-server",
  "version": "0.0.1",
  "private": "true",
  "main": "dist/src/server.js",
  "types": "dist/src/routes/index.d.ts",
  "scripts": {
    "start": "node dist/src/server.js",
    "dev": "concurrently \"ts-node-dev --respawn --transpile-only src/server.ts\" \"npm run build:watch\" \"npm run host:images\"",
    "build": "npm run build:common && tsc",
    "build:watch": "tsc --watch",
    "build:common": "cd ../common && npm ci && npm run build && cd ../server",
    "host:images": "http-server s3-mock -p 10000",
    "lint": "eslint --ext .ts --fix src",
    "test": "npm run build:common && jest"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.234.0",
    "@trpc/server": "^10.5.0",
    "common": "file:../common",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mustache": "^4.2.0",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.9.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.4",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mustache": "^4.2.2",
    "@types/nodemailer": "^6.4.7",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "concurrently": "^7.6.0",
    "eslint": "^8.30.0",
    "http-server": "^14.1.1",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  }
}
